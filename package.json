{
  "name": "1-prompt",
  "version": "1.0.3",
  "description": "Extract user prompts from AI chat platforms in One Click",
  "author": "Bharath Amaravadhi <Cursor Layout LLP>",
  "homepage": "https://1-prompt.in",
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=9.6.0"
  },
  "type": "module",
  "scripts": {
    "dev": "vite build --watch & vite build --config vite.content.config.ts --watch",
    "build": "rm -rf dist && tsc && vite build && vite build --config vite.background.config.ts && vite build --config vite.content.config.ts",
    "build:web": "npm run build && node scripts/prepare-web.js",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.268",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.9.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.3",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "wrangler": "^4.62.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-arm64": "*",
    "@rollup/rollup-linux-arm64-gnu": "*"
  }
}
